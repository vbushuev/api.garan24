{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "garan":"*",
        "garan-gateway-ariuspay":">=2.0",
        "woothemes/woocommerce-api": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories":[
        {
            "type":"package",
            "package":{
                "name":"garan",
                "version":"1.0",
                "source":{
                    "type":"git",
                    "url":"https://github.com/vbushuev/garan",
                    "reference":"release-1-0"
                },
                "autoload":{
                    "psr-4": {"Garan24\\": "src"}
                }
            }
        },
        {
            "type":"package",
            "package":{
                "name":"garan-gateway-ariuspay",
                "version":"2.0",
                "source":{
                    "type":"git",
                    "url":"https://github.com/vbushuev/gateway-ariuspay/",
                    "reference":"master"
                },
                "autoload":{
                    "psr-4": {"Garan24\\": "src"}
                }
            }
        }
    ]
}
