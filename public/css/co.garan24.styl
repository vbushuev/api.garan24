// out: co.garan24.css
@import "fonts.styl"
$fontColor = rgba(255,162,0,1)
$garanBlue = #2962FF//rgba(255,0,0,1)
$garanRed = #D50000//rgba(255,0,0,1)
//functions
border-radius()
   -webkit-border-radius arguments
   -moz-border-radius arguments
   border-radius arguments
background-transition()
   -webkit-transition opacity arguments ease-in//opacity 400ms ease-in;
   -moz-transition opacity arguments ease-in//opacity 400ms ease-in;
   transition  opacity arguments ease-in//opacity 400ms ease-in;
background-blur()
    -webkit-filter blur(arguments)
    -moz-filter blur(arguments)
    -o-filter blur(arguments)
    -ms-filter blur(arguments)
    filter blur(arguments)
//main styles
html,body
    height 100%
body
    font-weight 300
    font-family 'Open Sans'
    font-size 14pt
    margin 0
    padding 0
    width 100%
    .ui-dialog-titlebar
        display: none;
    /* Modal helper boxes*/
    .helper
        display: inline-block;
        background-color: rgba(85,125,161,1);
        color: rgba(255,255,255,.6);
        border-radius: (1em);
        min-width:2em;
        text-align: center;
        height: 2em;
        line-height: 2em;
        position: absolute;
        right: .5em;
        top: .5em;
        cursor:pointer;
        z-index: 100;
    .helper:hover
        color: rgba(255,255,255,1);
        text-decoration: none;
    .helper-text
        background-color: rgba(85,125,161,1);
        color: rgba(255,255,255,1);
        position: absolute;
        top: 3em;
        right: 1em;
        padding: 1em;
        z-index: 100;
        border-radius: (.5em);
    .helper-box
        display: none;
        position: fixed;
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        background: rgba(0,0,0,0.3);
        //background-blur(5px);
        z-index: 300;
        background-transition(400ms);
        pointer-events: none;
    .helper-box:target
        display: block;
        pointer-events: auto;
    .helper-box-auto
        position: fixed;
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        background: rgba(0,0,0,0.3);
        //background-blur(5px);
        z-index: 300;
        background-transition(400ms);
        pointer-events: auto;
    .helper-box-item-lg
        background-color: rgba(255,255,255,1);
        position: relative;
        z-index: 300;
        color: rgba(0,0,0,1);
        margin:4em 6em 1em 6em;
        height:92%
        padding: 4em;
        font-size: 12pt;
        text-indent: 2em;
        border-radius: (1em);
        overflow: auto;
    .helper-box-item
        background-color: rgba(255,255,255,1);
        display: none;
        position: relative;
        z-index: 300;
        color: rgba(0,0,0,1);
        width: 40em;
        margin:10% auto;
        padding: 4em;
        font-size: 12pt;
        text-indent: 2em;
        border-radius: (1em);
    .helper-box-item-close
        border: solid 4px transparent;
        position: fixed;
        top:1em;
        right:1em;
        color: rgba(255,255,255,1);
        border-radius: (1.5em);
        height: 3em;
        line-height: 3em;
        width:3em;
        text-align: center;
        display: inline-block;
        padding-bottom: .2em;
        text-indent: 0;
    .helper-box-item-close .fa
        color: rgba(255,255,255,1);
    .helper-box-item-close:hover
        border: solid 4px rgba(255,255,255,1);

    .container
        .first
            font-weight: 700;
            font-style: normal;
            font-size: inherit;
        h1,h2,h3
            font-weight: 500;
            text-transform: uppercase;
            margin-bottom: 1em;

        h1
            font-size: 20pt
            text-align: center;
            line-height: 2em;
            border-bottom: dotted 1px rgba(85,125,161,1);
        h2
            font-size: 18pt
        h3
            font-size: 16pt
            padding-right: 2em;
        h4
            font-weight: 700;
            font-size: 15pt;
        ul
            list-style: none;
            li
                display: inline-block;
                list-style-position: inside;
        .mail
            background-color: rgba(85,125,161,1);
            color: rgba(255,255,255,1);
            text-align: center;
            height: 3em;
            line-height: 3em;
        .row
            margin-left: 0;
            margin-right: 0;
        .brand
            color: rgba(85,125,161,1);
            font-weight: 700;
            text-transform: uppercase;
        .text
            text-indent: 2em;
        .data
            padding:1em;
            line-height: 2em;
            font-size: 110%;
        .bye
            margin: 1em auto;
            text-align: center;
            font-size: 110%;
        /*
        No labels
        */
        .control-label
            display: none;
        /*
        .btn-success
            background-color: #5cb85c;
            color: #fff;
            border-color: #4cae4c;
        .btn-info
            color: #fff;
            background-color: #5bc0de;
            border-color: #46b8da;
        .btn-primary
            color: #fff;
            background-color:  rgba(85,125,161,1);
            border-color: rgba(80,112,152,1);
        */
        input,select,textarea
            font-size: 14pt;

        .hide
            display: none;
        .small
            font-size: 10pt;
            a
                font-size: 10pt;
        .attention
            font-size: 110%;
            font-weight: 700;
            display: block;
            background-color: rgba(100,255,218 ,1);
            padding:.4em;
            text-align: center;


        .scale-2
            width: 100%;
        .scale-2-item
            position: relative;
            background-color: rgba(167,255,235 ,1);
            display: inline-block;
            width: 25%;
            text-align: center;
            height: 64px;
            line-height: 64px;
            border:none;
            font-size: 16pt;
            color: rgba(0,0,0,.3);
            pointer-events: none;
        .scale-2-item a
            color: rgba(0,0,0,.3);
            text-decoration: none;
        .scale-2-item:after, .scale-2-item:before
            top: 50%;
            border: solid transparent;
            content: " ";
            height: 0;
            width: 0;
            position: absolute;
            pointer-events: none;
        .scale-2-item:after
            border-color: transparent;
            border-left-color: rgba(167,255,235 ,1);
            border-width: 32px;
            margin-top: -32px;
            right: -60px;
            z-index: 200;
        .scale-2-item:before
            border-color: transparent;
            border-left-color: rgba(255,255,255,1);
            border-width: 32px;
            z-index: 100;
            left:0;
            margin-top: -32px;
        .scale-2-item:first-child:before
            border-left-color: rgba(167,255,235 ,1);
        .scale-2-item:last-child:after
            //border-left-color: rgba(255,255,255,1);
            border-left-color: transparent;
        .active.scale-2-item,.active-last.scale-2-item
            background-color: #5cb85c;//#449d44
            color: rgba(255,255,255,1);
        .active.scale-2-item a,.active-last.scale-2-item a
            color: rgba(255,255,255,1);
        .active-last.scale-2-item
            pointer-events: all;
        .active.scale-2-item,.active-last.scale-2-item a:hover
            font-weight: 700;
            text-transform: uppercase;
        .active-last.scale-2-item:after,.active-last.scale-2-item:first-child:before,
        .active.scale-2-item:after,.active.scale-2-item:first-child:before
            border-left-color: #5cb85c;
        .active.scale-2-item:last-child:after,
        .active-last.scale-2-item:last-child:after{
            border-left-color: transparent;
        }
        .form
            position: relative;
            .fa
                color: rgba(85,125,161,1);
            .helper .fa
                color: rgba(255,255,255,.6);
            .helper:hover .fa
                color: rgba(255,255,255,1);

        .input-group-addon
            background-color: rgba(0,0,0,0);
            .fa
                color: rgba(85,125,161,1);
        /* Cart*/
        .cart
            border:dotted 1px rgba(100,255,218 ,1);
            padding: 0 1em;
        .cart-item
            margin-bottom: 1.4em;
            cursor: pointer;
            font-size: 12pt;
            *
                margin: 0;
                padding:0;

            .image
                display: inline-block;
                text-align: center;
                img
                    height: 3em;
                    width: 3em;
            .name
                padding-left: 0em;
            .quantity
                font-weight: 700;
            .amount
                text-align: right;
                font-weight: 700;
                font-family: 'Lato', 'Open Sans';
            .total
                text-align: right;
        #cart-total
            border-top:dotted 1px rgba(85,125,161,1);
            margin-top: 2.4em;
        #cart-shipping
            border: none;
        #cart-shipping-total
            border-top:dotted 1px rgba(85,125,161,1);
            margin-top: 1em;
            color: rgba(85,125,161,1);
            *
                font-size: 16pt;
                font-weight: 700;

        .list-group-item-text
            text-indent: 1.4em;
        .list-group-item.active .fa
            color: rgba(255,255,255,1);

        #header
            font-size: 12pt
            padding: 0;
            .nav
                background-color rgba(85,125,161,1)
                list-style: none;
                color: rgba(255,255,255,.8);
                text-align: right;
                padding-bottom: .5em;
                .nav-item
                    height: 2em;
                    line-height: 2em;
                    display: inline-block;
                    font-size: 12pt;
                    a
                        color rgba(174,213,129 ,1)
                        font-size: 12pt;
                    a:hover
                        background: none;
                        text-decoration: underline;
                .nav-item.logo
                    float:left;
                    font-size: 16pt;
                    margin-left: 1em;
        #content
            background-color rgba(0,0,0,0)
            .cart
                padding-right: 2em;
                margin-bottom: 2em;

            .message
                background-color: rgba(185,246,202 ,1);
                border-radius(.5em);
                padding:1em;
                font-size: 12pt;
            .message-alert
                background-color: rgba(233,30,99 ,1);
                color: rgba(255,255,255,1);
                text-indent: 1em;
        #footer
            margin-top: 2em;

            color:#fff;
            background-color rgba(55,71,79 ,1);
            text-align: center;
