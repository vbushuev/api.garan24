// out: co.css
@import "fonts.styl"
$fontColor = rgba(255,162,0,1)
$garanBlue = #2962FF//rgba(255,0,0,1)
$garanRed = #D50000//rgba(255,0,0,1)
//functions
border-radius()
   -webkit-border-radius arguments
   -moz-border-radius arguments
   border-radius arguments
background-transition()
   -webkit-transition opacity arguments ease-in//opacity 400ms ease-in;
   -moz-transition opacity arguments ease-in//opacity 400ms ease-in;
   transition  opacity arguments ease-in//opacity 400ms ease-in;
background-blur()
    -webkit-filter blur(arguments)
    -moz-filter blur(arguments)
    -o-filter blur(arguments)
    -ms-filter blur(arguments)
    filter blur(arguments)
//main styles
html,body
    height 100%
body
    font-weight 300
    font-family 'Open Sans'
    font-size 14pt
    margin 0
    padding 0
    width 100%
    .ui-dialog-titlebar
        display: none;
    .container
        .first
            font-weight: 700;
            font-style: normal;
            font-size: inherit;
        h1,h2,h3
            font-weight: 500;
            text-transform: uppercase;
            margin-bottom: 1em;

        h1
            font-size: 20pt
            text-align: center;
            height:2em;
            line-height: 2em;
            border-bottom: dotted 1px rgba(85,125,161,1);
        h2
            font-size: 18pt
        h3
            font-size: 16pt
        h4
            font-weight: 700;
            font-size: 15pt;
        ul
            list-style: none;
            li
                display: inline-block;
                list-style-position: inside;
        .row
            margin-left: 0;
            margin-right: 0;
        /*
        No labels
        */
        .control-label
            display: none;

        input,select,textarea
            font-size: 14pt;

        .hide
            display: none;
        .small
            font-size: 10pt;
            a
                font-size: 10pt;
        .attention
            font-size: 110%;
            font-weight: 700;
            display: block;
            background-color: rgba(100,255,218 ,1);
            padding:.4em;
            text-align: center;
        .scale
            width: 100%;
            padding:0;
            margin: 0;
            .scale-item
                display: inline-block;
                font-size: 10pt;
                width:25%;
                margin: 0;
                height: 3em;
                line-height: 3em;
                text-align: center;
                position: relative;
                border: dotted 1px rgba(174,213,129 ,1);
                color: rgba(0,0,0,.3);
            .scale-item.active
                background-color: rgba(174,213,129 ,1);
                color: rgba(255,255,255,1);
            .scale-item.active::after
                content: '';
                position: absolute;
                right: -3em; top: 0;
                border: 1.5em solid transparent;
                border-left: 1.5em solid rgba(174,213,129 ,1);*/
