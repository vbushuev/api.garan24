        .form
            position: relative;
            .fa
                color: rgba(85,125,161,1);
            .helper .fa
                color: rgba(255,255,255,.6);
            .helper:hover .fa
                color: rgba(255,255,255,1);

        /* Modal helper boxes*/
        .helper
            display: inline-block;
            background-color: rgba(85,125,161,1);
            color: rgba(255,255,255,.6);
            border-radius: (1em);
            min-width:2em;
            text-align: center;
            height: 2em;
            line-height: 2em;
            position: absolute;
            right: .5em;
            top: .5em;
            cursor:pointer;
            z-index: 100;
        .helper:hover
            color: rgba(255,255,255,1);
            text-decoration: none;
        .helper-text
            background-color: rgba(85,125,161,1);
            color: rgba(255,255,255,1);
            position: absolute;
            top: 3em;
            right: 1em;
            padding: 1em;
            z-index: 100;
            border-radius: (.5em);
        .helper-box
            position: fixed;
            top: 0;
            right: 0;
            bottom: 0;
            left: 0;
            background: rgba(0,0,0,0.3);
            //background-blur(5px);
            z-index: 9998;
            background-transition(400ms);
            display: none;
            pointer-events: none;
        .helper-box:target
            display: block;
            pointer-events: auto;

        .helper-box-item
            background-color: rgba(255,255,255,1);
            display: none;
            position: relative;
            z-index: 9999;
            color: rgba(0,0,0,1);
            width: 40em;
            margin:10% auto;
            padding: 4em;
            font-size: 12pt;
            text-indent: 2em;
            border-radius: (1em);
        .helper-box-item-close
            border: solid 1px rgba(255,255,255,1);
            position: absolute;
            top:.4em;
            right:.4em;
            color: rgba(0,0,0,1);
            border-radius: (1em);
            height: 1.4em;
            line-height: 1.4em;
            width:1.4em;
            text-align: center;
            display: inline-block;
            padding-bottom: .2em;
            text-indent: 0;
        .helper-box-item-close .fa
            color: rgba(0,0,0,1);
        .helper-box-item-close:hover
            border: solid 1px rgba(0,0,0,1);
        .input-group-addon
            background-color: rgba(0,0,0,0);
            .fa
                color: rgba(85,125,161,1);
        /* Cart*/
        .cart
            border:dotted 1px rgba(100,255,218 ,1);
            padding: 0 1em;
        .cart-item
            margin-bottom: 1.4em;
            cursor: pointer;
            font-size: 12pt;
            *
                margin: 0;
                padding:0;

            .image
                display: inline-block;
                text-align: center;
                img
                    height: 3em;
                    width: 3em;
            .name
                padding-left: 0em;
            .quantity
                font-weight: 700;
            .amount
                text-align: right;
                font-weight: 700;
                font-family: 'Lato', 'Open Sans';
            .total
                text-align: right;
        #cart-total
            border-top:dotted 1px rgba(85,125,161,1);
            margin-top: 2.4em;
        #cart-shipping
            border: none;
        #cart-shipping-total
            border-top:dotted 1px rgba(85,125,161,1);
            margin-top: 1em;
            color: rgba(85,125,161,1);
            *
                font-size: 16pt;
                font-weight: 700;

        .list-group-item-text
            text-indent: 1.4em;
        .list-group-item.active .fa
            color: rgba(255,255,255,1);

        #header
            font-size: 12pt
            padding: 0;
            .nav
                background-color rgba(85,125,161,1)
                list-style: none;
                color: rgba(255,255,255,.8);
                text-align: right;
                padding-bottom: .5em;
                .nav-item
                    height: 2em;
                    line-height: 2em;
                    display: inline-block;
                    font-size: 12pt;
                    a
                        color rgba(174,213,129 ,1)
                        font-size: 12pt;
                    a:hover
                        background: none;
                        text-decoration: underline;
        #content
            background-color rgba(0,0,0,0)
            .cart
                padding-right: 2em;
                margin-bottom: 2em;

            .message
                background-color: rgba(185,246,202 ,1);
                border-radius(.5em);
                padding:1em;
                font-size: 12pt;
            .message-alert
                background-color: rgba(233,30,99 ,1);
                color: rgba(255,255,255,1);
                text-indent: 1em;
        #footer
            margin-top: 2em;

            color:#fff;
            background-color rgba(55,71,79 ,1);
            text-align: center;
            p,a
                font-size: 10pt;
